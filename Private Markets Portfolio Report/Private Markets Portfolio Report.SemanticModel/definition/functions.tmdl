function 'HextPoint.PowerBI.CumulativeToDate' = ```
		(MeasureToCumulative : expr) =>
			
		VAR MaxDate = MAX('Dates'[Date])
		VAR MinDateSelected = 
		    CALCULATE(
		        MIN('Dates'[Date]),
		        ALLSELECTED('Dates')
		    )
		
		VAR Result =
		    CALCULATE(
		        MeasureToCumulative,
		        DATESBETWEEN('Dates'[Date],MinDateSelected,MaxDate)
		    )
		
		RETURN Result
		```
	lineageTag: 632d67ea-b4c4-4c9e-93dd-870f6f6b7589

function 'HextPoint.PowerBI.ClosingValue' = ```
		(ClosingExpr : expr) =>
			
		VAR MaxDateInFilter = MAX('Dates'[Date])
		VAR MaxDate = TREATAS({MaxDateInFilter}, 'Dates'[Date])
		
		VAR Calc = CALCULATE(
		    ClosingExpr,
		    MaxDate
		)
		
		VAR CheckDateWindow = NOT(ISEMPTY('Investment Metrics'))
		
		VAR Result = IF(ISBLANK(Calc),IF(CheckDateWindow,0,BLANK()),Calc)
		
		RETURN Result
		```
