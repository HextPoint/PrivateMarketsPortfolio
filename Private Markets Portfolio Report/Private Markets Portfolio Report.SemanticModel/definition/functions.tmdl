function 'HextPoint.PowerBI.InvestmentMetricInvestmentCurrencyFormat' = ```
		() =>
			
		VAR InvestmentCurrencyKeyUnique = HASONEVALUE('Investment Metrics'[InvestmentCurrencyKey])
		
		VAR CurrentInvestmentCurrencyKey = IF(InvestmentCurrencyKeyUnique, FIRSTNONBLANK('Investment Metrics'[InvestmentCurrencyKey],'Investment Metrics'[InvestmentCurrencyKey]),BLANK())
		
		VAR Result = IF(InvestmentCurrencyKeyUnique,CALCULATE(
		        FIRSTNONBLANK('Investment Currencies'[Currency format string],'Investment Currencies'[Currency format string]),
		        'Investment Currencies'[CurrencyKey] = CurrentInvestmentCurrencyKey),"#,0;(#,0);#,0")
		
		RETURN Result
		```
	lineageTag: 69c00bc1-c9e5-431f-8077-b92c46bc112b

function 'HextPoint.PowerBI.TransactionCurrencyFormat' = ```
		() =>
			
