		VAR CurrentInvestmentCurrencyKey = IF(InvestmentCurrencyKeyUnique, FIRSTNONBLANK('Transactions'[CurrencyKey],'Transactions'[CurrencyKey]),BLANK())
		
		VAR Result = IF(InvestmentCurrencyKeyUnique,CALCULATE(
		        FIRSTNONBLANK('Investment Currencies'[Transaction format string],'Investment Currencies'[Transaction format string]),
